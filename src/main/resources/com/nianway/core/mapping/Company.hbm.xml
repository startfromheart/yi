<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.nianway.core.mapping">
	<class name="Company" table="COMPANYS">
		<id name="companyId" column="companyId">
			<generator class="uuid.hex" />
		</id>
		<property name="companyName" column="companyName" />
		<property name="companyIntroduce" column="companyIntroduce" />
		<property name="state" column="state" type="integer"/>
	</class>
</hibernate-mapping>